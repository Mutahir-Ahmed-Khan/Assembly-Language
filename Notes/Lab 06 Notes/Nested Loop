----------------------------------------------------
	mov eax, 1 
	mov ecx, 5 

	OuterLoop: 
		mov counter, ecx  //Storing ecx in the counter 
		mov ecx, 10	  // Modifying it for inner Loop
		
		InnerLoop:  
			call WriteDec
			call Crlf
			inc eax
			loop InnerLoop

		call Crlf
		mov ecx, counter  //Restoring the Counter
		loop OuterLoop	//For Inner Loop
------------------------------------------------------
